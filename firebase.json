{
  "storage": {
    "rules": "storage.rules",
    "cors": [
      {
        "origin": ["*"],
        "method": ["GET", "PUT", "POST", "DELETE", "HEAD", "OPTIONS"],
        "maxAgeSeconds": 3600,
        "responseHeader": ["Content-Type", "Authorization", "Content-Length", "User-Agent", "X-Requested-With", "Accept", "Origin", "x-goog-resumable"]
      }
    ]
  }
}
